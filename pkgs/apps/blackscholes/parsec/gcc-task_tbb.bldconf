#!/bin/bash
BASEPATH="$(cd $(dirname $BASH_SOURCE); pwd)"
source ${BASEPATH}/gcc.bldconf
del_ver_build_env=`echo "${build_env}"|sed -e "s/^version=[a-zA-Z_0-9]\+ //"|sed -e "s/ version=[a-zA-Z_0-9]\+//"|sed -e "s/^version=[a-zA-Z_0-9]\+$//"`
build_env="${del_ver_build_env} version=task task_target=tbb platform=g"

